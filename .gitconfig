[user]
	name = Jared Jensen
	email = j@redj.xyz
[push]
	default = simple
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[branch]
	autosetupmerge = true
	autosetuprebase = always
[clean]
	requireForce = false
